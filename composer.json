{
    "name": "wanghanlin/laravel-queue-pool",
    "description": "A Laravel package provide pool command to start multiple queue worker all at once.",
    "keywords": [
        "laravel",
        "laravel-queue-pool"
    ],
    "homepage": "https://github.com/wanghanlin/laravel-queue-pool",
    "license": "MIT",
    "authors": [
        {
            "name": "Hanlin Wang",
            "email": "605231181@qq.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/console": "5.5.*|5.6.*",
        "illuminate/queue": "5.5.*|5.6.*",
        "illuminate/support": "5.5.*|5.6.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Wanghanlin\\QueuePool\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wanghanlin\\QueuePool\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wanghanlin\\QueuePool\\QueuePoolServiceProvider"
            ]
        }
    }
}
